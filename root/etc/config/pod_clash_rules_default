config rule
  option rule_type 'DOMAIN-SUFFIX'
  option matcher 'google.com'
  option proxies 'auto'

config rule_provider
  option name 'Apple'
  option type 'http'
  option behavior 'classical'
  option url_path 'https://raw.githubusercontent.com/lhie1/Rules/master/Clash/Provider/Apple.yaml'
  option interval '300'